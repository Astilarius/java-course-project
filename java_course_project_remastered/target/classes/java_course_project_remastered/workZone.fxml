<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="java_course_project_remastered.WorkZoneController">
    <children>
        <Label text=""  fx:id="projectInfo"/>
        <HBox alignment="CENTER" spacing="20.0">
            <children>
                <Button text="Выйти и сохранить" onAction="#exitSave"/>
                <Button text="сохранить" onAction="#save"/>
                <Button text="Добавить материал" onAction="#addMat"/>
                <Button text="Добавить производство" onAction="#addProd"/>
            </children>
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </padding>
        </HBox>
        <FlowPane fx:id="workZoneFP" alignment="CENTER">
            <children>
            </children>
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </padding>
        </FlowPane>
        <Label text=""  fx:id="msg"/>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
